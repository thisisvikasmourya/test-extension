{
  "manifest_version": 3,
  "name": "Parkour Ai Extension",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon128.png"
    }
  },
  "permissions": ["activeTab"],
  "content_script": {
    "content": "content.js"
  }
}
